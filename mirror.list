set base_path         /var/spool/apt-mirror
set mirror_path       $base_path/mirror
set skel_path         $base_path/skel
set var_path          $base_path/var
set postmirror_script $var_path/postmirror.sh
set defaultarch       amd64
set run_postmirror    0
set nthreads          6
set limit_rate        100m
set _tilde            0
# Use --unlink with wget (for use with hardlinked directories)
set unlink            1
set use_proxy         off
set http_proxy        127.0.0.1:3128
set proxy_user        user
set proxy_password    password

deb http://ftp.halifax.rwth-aachen.de/ubuntu jammy multiverse
deb http://ftp.halifax.rwth-aachen.de/ubuntu jammy-updates multiverse
deb http://ftp.halifax.rwth-aachen.de/ubuntu jammy main restricted
deb http://ftp.halifax.rwth-aachen.de/ubuntu jammy universe
deb http://ftp.halifax.rwth-aachen.de/ubuntu jammy-updates universe
deb http://ftp.halifax.rwth-aachen.de/ubuntu jammy-backports main restricted universe multiverse

deb [arch=arm64] http://ftp.halifax.rwth-aachen.de/ubuntu jammy multiverse
deb [arch=arm64] http://ftp.halifax.rwth-aachen.de/ubuntu jammy-updates multiverse
deb [arch=arm64] http://ftp.halifax.rwth-aachen.de/ubuntu jammy main restricted
deb [arch=arm64] http://ftp.halifax.rwth-aachen.de/ubuntu jammy universe
deb [arch=arm64] http://ftp.halifax.rwth-aachen.de/ubuntu jammy-updates universe
deb [arch=arm64] http://ftp.halifax.rwth-aachen.de/ubuntu jammy-backports main restricted universe multiverse

clean http://ftp.halifax.rwth-aachen.de/ubuntu

deb https://mirror.i3d.net/debian bookworm main contrib non-free non-free-firmware
deb https://mirror.i3d.net/debian bookworm-updates main contrib non-free non-free-firmware
deb [arch=arm64] https://mirror.i3d.net/debian bookworm main contrib non-free non-free-firmware
deb [arch=arm64] https://mirror.i3d.net/debian bookworm-updates main contrib non-free non-free-firmware

clean  https://mirror.i3d.net/debian

deb https://pkgs.tailscale.com/stable/ubuntu jammy main
deb [arch=arm64] http://archive.raspberrypi.com/debian bookworm main


